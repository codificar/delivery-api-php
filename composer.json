{
    "name": "codificar/delivery-api-php",
    "description": "Delivery Codificar PHP Library",
    "type": "lib",
    "keywords": [
      "codificar",
      "delivery",
      "api"
    ],
    "license": "MIT",
    "require": {
      "php": ">=5.6.0",
      "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
      "phpunit/phpunit": "^8",
      "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
      "test": [
        "@composer install",
        "vendor/bin/phpunit"
      ]
    },
    "autoload": {
      "psr-4": {"Delivery\\": "src/"}
    },
    "autoload-dev": {
      "psr-4": {
        "Delivery\\Test\\": "tests/unit"
      }
    }
  }
